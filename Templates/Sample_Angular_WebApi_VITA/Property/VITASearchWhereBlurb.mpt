	case 14: // CharUnicode
		<%%=Solution.NEWLINE%%>
		if (Length > 1)
		{
			<%%-.AndIfNotEmpty(query.%%><%%=VITAPropertyName%%><%%-, i => i%%><%%=baseEntityPath%%><%%-.%%><%%=VITAPropertyPath%%><%%-.StartsWith(query.%%><%%=VITAPropertyName%%><%%-))%%>
