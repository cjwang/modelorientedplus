		{
			<%%-.AndIfNotEmpty(query.%%><%%=VITAPropertyName%%><%%-, i => i%%><%%=baseEntityPath%%><%%-.%%><%%=VITAPropertyPath%%><%%- == query.%%><%%=VITAPropertyName%%><%%-.Value)%%>
