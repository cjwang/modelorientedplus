		<%%=Solution.NEWLINE%%>
		<%%-.AndIf(query.%%><%%=VITAPropertyName%%><%%- != %%><%%=CSharpDefaultValue%%><%%-, i => i%%><%%=baseEntityPath%%><%%-.%%><%%=VITAPropertyPath%%><%%- == query.%%><%%=VITAPropertyName%%><%%-.Value)%%>
